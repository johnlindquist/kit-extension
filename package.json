{
  "name": "kit-extension",
  "displayName": "Script Kit",
  "description": "Run current script in Kit.app",
  "version": "0.2.5",
  "publisher": "johnlindquist",
  "private": true,
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnlindquist/kit-extension"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "keywords": [
    "ScriptKit",
    "Script",
    "Kit",
    "Script Kit"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kit.run",
        "title": "Run in Kit.app"
      }
    ],
    "keybindings": [
      {
        "command": "kit.run",
        "key": "alt+ctrl+;",
        "mac": "alt+cmd+;"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.13.0",
    "typescript": "^4.7.2"
  }
}
